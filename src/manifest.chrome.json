{
  "manifest_version": 2,
  "name": "DSW",
  "description": "Deny Service Worker",
  "version": "0.1.0",
  "default_locale": "en",
  
  "author": "Kenrick",
  "homepage_url": "https://www.kenrick95.org/",
  
  "background": {
    "page": "/html/background.html"
  },
  
  "options_ui": {
    "page": "/html/options.html"
  },
  
  "browser_action": {
    "default_title": "DSW"
  },
  
  "content_scripts": [{
    "all_frames": true,
    "matches": ["https://*/*"],
    "js": ["/js/content.js"],
    "run_at": "document_start"
  }],
  
  "permissions": [
    "activeTab",
    "storage"
  ]
}